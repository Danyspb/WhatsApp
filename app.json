{
  "expo": {
    "name": "WhatsApp_Clone",
    "slug": "WhatsApp_Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whatsap.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/whatsapp.png",
      "resizeMode": "cover",
      "backgroundColor": "#68B984"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/whatsap.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
